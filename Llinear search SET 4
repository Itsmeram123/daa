#include <stdio.h>
int main()
{
  int array[100], i, search, n;
  printf("Enter number of elements: ");
  scanf("%d", &n);
  printf("Enter array elements:\n");
  for ( i= 0; i < n; i++)
    scanf("%d", &array[i]);
  printf("Enter elements to search:");
  scanf("%d", &search);
  for (i = 0; i < n; i++)
    if (array[i] == search) 
	break;
	if(i<n)
	printf("Elements found at index %d",i);
	else  
      printf("Element not found");
    return 0;
}
 
